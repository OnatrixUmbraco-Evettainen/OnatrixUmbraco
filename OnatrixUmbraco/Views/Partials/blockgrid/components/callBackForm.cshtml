@using OnatrixUmbraco.ViewModels
@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<Umbraco.Cms.Core.Models.Blocks.BlockGridItem>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;

@{
    var formTitle = Model.Content.Value<string>("formTitle");
    var formDescription = Model.Content.Value<string>("formDescription");

    var viewModel = new FormCallBackViewModel();
}

<div class="call-back-form">
    <div class="form-header">
        <p class="text-lead fw-bold">@formTitle</p>
    </div>
    <p class="text-xs fst-italic">@formDescription</p>
    @await Html.PartialAsync("~/views/partials/forms/_formCallBack.cshtml", viewModel)
</div>